a = 7  
b = 7.3 
c = True 
d = "안녕" 
e = '7' 


print(a+1)
print(b)
print(c)
print(d + str(a))
print(int(e)+1)



#Python 문자열 위치
#숫자로 표현. 
f = "Life is too short, You need Python"

print("첫번재 문자 f[0] : " + f[0])
print("마지막 문자 f[-1] : " + f[-1])

#문자열 슬라이싱
g = f[0]+f[1]+f[2]+f[3]
print(g)
print(f[0:4])


