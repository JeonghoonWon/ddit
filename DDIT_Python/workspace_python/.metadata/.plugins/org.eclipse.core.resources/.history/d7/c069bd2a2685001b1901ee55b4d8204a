import sys
from PyQt5.QtWidgets import *
from PyQt5 import uic, QtGui, QtCore


form_class = uic.loadUiType("myqt01_1.ui")[0]

class MyWindow(QMainWindow, form_class):
    def __init__(self):
        super().__init__()
        self.setupUi(self)
       
        pb = QPushButton(self)
        pb.set.Icon(QtGui.QIcon("0.png"))
        pb.setIcoSize(QtCore.QSize(40,40))
        pb.setGeometry(QtCore.QRect(40,0,40,40))
        
    
    
   
    
    def myclick(self):
        size = 40
        
        

if __name__ == "__main__":
    app = QApplication(sys.argv)
    myWindow = MyWindow()
    myWindow.show()
    app.exec_()